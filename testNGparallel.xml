<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="MusalaSoftAutomation" parallel="tests" thread-count="6">

    <test name="SignInMusalaMasters-chrome">

        <parameter name="browser" value="Chrome" />
        <parameter name="baseURL" value="https://masters.musala.com/" />

        <classes>
            <class name="com.musala.selenium.tests.ui_tests.sign_in.InvalidSignIn">
            </class>
        </classes>
    </test>

    <test name="SignInMusalaMasters-firefox">

        <parameter name="browser" value="Firefox" />
        <parameter name="baseURL" value="https://masters.musala.com/" />

        <classes>
            <class name="com.musala.selenium.tests.ui_tests.sign_in.InvalidSignIn">
            </class>
        </classes>
    </test>

    <test name="FacebookConference-chrome">

        <parameter name="browser" value="Chrome" />
        <parameter name="baseURL" value="https://masters.musala.com/" />

        <classes>
            <class name="com.musala.selenium.tests.ui_tests.facebook.NavigateToMuffinConference">
            </class>
        </classes>
    </test>

    <test name="FacebookConference-firefox">

        <parameter name="browser" value="Firefox" />
        <parameter name="baseURL" value="https://masters.musala.com/" />

        <classes>
            <class name="com.musala.selenium.tests.ui_tests.facebook.NavigateToMuffinConference">
            </class>
        </classes>
    </test>

    <test name="EventSchedule-chrome">

        <parameter name="browser" value="Chrome" />
        <parameter name="baseURL" value="https://masters.musala.com/" />

        <classes>
            <class name="com.musala.selenium.tests.ui_tests.events.MasterClassEventSchedule">
            </class>
        </classes>
    </test>

    <test name="EventSchedule-firefox">

        <parameter name="browser" value="Firefox" />
        <parameter name="baseURL" value="https://masters.musala.com/" />

        <classes>
            <class name="com.musala.selenium.tests.ui_tests.events.MasterClassEventSchedule">
            </class>
        </classes>
    </test>

</suite>